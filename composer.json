{
    "name": "fasync/event-loop",
    "description": "Fiber native eventloop for building block for heart of fasync libraries",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fasync\\EventLoop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Reymart A. Calicdan"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "pestphp/pest": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}